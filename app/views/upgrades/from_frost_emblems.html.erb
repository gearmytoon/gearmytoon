<div id="upgrades">
  <div class="upgrade_section" id="emblem_of_frost">
    <h1>Your best upgrades from <%= stylized_item_name Item.badge_of_triumph %> <%= wowhead_item_image_link Item.badge_of_triumph %></h1>
    <% @character.frost_upgrades.each do |upgrade| %>
      <%= render 'shared/upgrade', :upgrade => upgrade %>
    <% end %>
  </div>
</div>
